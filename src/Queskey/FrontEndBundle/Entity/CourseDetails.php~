<?php

namespace Queskey\FrontEndBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CourseDetails
 */
class CourseDetails
{
    /**
     * @var string
     */
    private $category;

    /**
     * @var string
     */
    private $courseName;

    /**
     * @var string
     */
    private $courseImage;

    /**
     * @var string
     */
    private $courseProvider;

    /**
     * @var string
     */
    private $courseProviderImg;

    /**
     * @var integer
     */
    private $coursePrice;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set category
     *
     * @param string $category
     * @return CourseDetails
     */
    public function setCategory($category)
    {
        $this->category = $category;

        return $this;
    }

    /**
     * Get category
     *
     * @return string 
     */
    public function getCategory()
    {
        return $this->category;
    }

    /**
     * Set courseName
     *
     * @param string $courseName
     * @return CourseDetails
     */
    public function setCourseName($courseName)
    {
        $this->courseName = $courseName;

        return $this;
    }

    /**
     * Get courseName
     *
     * @return string 
     */
    public function getCourseName()
    {
        return $this->courseName;
    }

    /**
     * Set courseImage
     *
     * @param string $courseImage
     * @return CourseDetails
     */
    public function setCourseImage($courseImage)
    {
        $this->courseImage = $courseImage;

        return $this;
    }

    /**
     * Get courseImage
     *
     * @return string 
     */
    public function getCourseImage()
    {
        return $this->courseImage;
    }

    /**
     * Set courseProvider
     *
     * @param string $courseProvider
     * @return CourseDetails
     */
    public function setCourseProvider($courseProvider)
    {
        $this->courseProvider = $courseProvider;

        return $this;
    }

    /**
     * Get courseProvider
     *
     * @return string 
     */
    public function getCourseProvider()
    {
        return $this->courseProvider;
    }

    /**
     * Set courseProviderImg
     *
     * @param string $courseProviderImg
     * @return CourseDetails
     */
    public function setCourseProviderImg($courseProviderImg)
    {
        $this->courseProviderImg = $courseProviderImg;

        return $this;
    }

    /**
     * Get courseProviderImg
     *
     * @return string 
     */
    public function getCourseProviderImg()
    {
        return $this->courseProviderImg;
    }

    /**
     * Set coursePrice
     *
     * @param integer $coursePrice
     * @return CourseDetails
     */
    public function setCoursePrice($coursePrice)
    {
        $this->coursePrice = $coursePrice;

        return $this;
    }

    /**
     * Get coursePrice
     *
     * @return integer 
     */
    public function getCoursePrice()
    {
        return $this->coursePrice;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
